---
import Layout from "../layouts/Layout.astro";

import { profile } from "@/settings";
import { Projets } from "@/data/cv";
import Hero from "@/components/ui/Hero.astro";
import ProjectCard from "@/components/ui/ProjectCard.astro";

const { fullName, title, institute } = profile;
---

<Layout>
	<!-- Hero Section -->
	<section class="flex items-center gap-12 border-b pb-12">
				<Hero
					fullName={fullName}
					title={title}
					institute={institute}
					profilePicture={ProfilePictur}
				/>
	</section>

	<!-- Projet liist -->
	<section class="py-6 border-b">
		<h2 class="text-3xl font-bold mb-8">Projets RÃ©aliser</h2>
		<div
			class="flex justify-center items-center left-0 right-0 top-0 bottom-0 w-full"
		>
			<div
				class="py-4 grid grid-cols-1 md:grid-cols-2 lg:grid-cols-2 xl:grid-cols-3 gap-9 px-6 items-center"
			>
				{Projets.map((proj) => <ProjectCard {...proj} />)}
			</div>
		</div>
	</section>

	<!-- Skills -->
	<section class="mb-4">
		<h2 class="text-2xl font-bold mb-6">Competances</h2>
		<fieldset
			class="fieldset border-base-300 rounded-box w-xs border p-4 mb-3"
		>
			<legend class="fieldset-legend font-semibold italic"
				>En programmation
			</legend>

